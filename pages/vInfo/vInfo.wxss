/* pages/info/info.wxss */

.video_box{
    width: 100%;
    overflow: hidden;
    position: relative;
}
.video_box video{
    width: 100%;
}

.video_action_box{
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    overflow: hidden;
    border-bottom: 10rpx solid #fafafa;
}
.video_action_box li{
    flex: 1;
    flex-wrap: wrap;
    font-size:24rpx;
    line-height: 100rpx;
}

.video_action_box li .iconfont{
    flex: 1;
    flex-wrap: wrap;
    line-height: 1rem;
    margin-right: 10rpx;
}

.video_info_box{
    padding: 20rpx;
    border-bottom: 10rpx solid #fafafa;
}
.video_info_box .title {
    display: block;
    line-height: 40rpx;
    font-size: 36rpx;
    font-weight: 500;
    color: #333;
    margin-bottom: 20rpx;
}
.video_info_box .page_view {
    position: relative;
    margin-bottom: 20rpx;
    color: #999;
}
.video_info_box .page_view .iconfont {
     font-size: 32rpx;
}

.video_info_box .page_view .left_data {
    display: inline-block;
}
.video_info_box .page_view .left_data .item{
    display: inline-block;
    margin-right: 40rpx;
}
.video_info_box .page_view .open_icon {
    position: absolute;
    right: 0;
    top: -5rpx;
    font-size: 40rpx !important;
}

.video_info_box .info_box {
    height: 40rpx;
    line-height: 40rpx;
    overflow: hidden;
    transition: .5s all;
}

.video_info_box .info_box .info {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #999;
}

.video_info_box .info_box .crumbs_nav {
    margin-top: 40rpx;
    color: #999;
}

.video_info_box .info_box .crumbs_nav navigator {
    display: inline-block;
    font-size: 28rpx;
    color: #8fdac6;
    margin-right: 10rpx;
}

.video_info_box .info_box .crumbs_nav navigator:after {
    display: inline-block;
    font-size: 28rpx;
    content: "\E617";
    color: #999;
    font-family: "iconfont";
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}



.video_box .load_play{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top:0;
    z-index: 9999;
    background: #000000;
    background-size: cover;
}
.video_box .load_play .iconfont{
    font-size: 150rpx;
    color: #fff;
    position: absolute;
    bottom: 40rpx;
    right: 40rpx;
}
.video_box .load_play .video_time{
    position: absolute;
    left: 40rpx;
    bottom: 40rpx;
    border: 1rpx solid hsla(0,0%,100%,.6);
    background-color: hsla(0,0%,100%,.2);
    padding: 10rpx 20rpx;
    border-radius: 10rpx;
    color: #fff;
}












.comment_box{
    overflow: hidden;
    border-bottom: 10rpx solid #fafafa;
}
.comment_box .title {
    padding: 0 20rpx;
    margin-top: 20rpx;
}

.comment_box .comment_list {
    padding: 0 20rpx;
    overflow: hidden;
}
.comment_box .comment_list .item {
    display: block;
    position: relative;
    overflow: hidden;
    border-bottom: 1rpx solid #fafafa;
    padding: 40rpx 0 40rpx 100rpx;
    color: #999;
}

.comment_list .item .user_pic {
    position: absolute;
    top: 40rpx;
    left: 0;
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border-radius: 50%;
}

.comment_list .item .user_name_box {
    max-width: 60%;
    color: #999;
    display: block;
    margin-bottom: 20rpx;
}

.comment_list .item .praise {
    position: absolute;
    top: 40rpx;
    right: 0;
    color: #999;
}
.comment_list .item .user_name_box .user_name {
    margin-bottom: 5rpx;
}
.comment_list .item .user_name_box .user_time {
    font-size: 24rpx
}
.comment_list .item .text {
    color: #666;
}
.comment_list .item .praise .iconfont {
    font-size: 32rpx;
    margin-right: 10rpx;
}


.comment_box .more_comment_btn {
    margin: 40rpx 0;
    display: block;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 28rpx;
    color: #999;
    text-align: center;
}








.related_video_box {
    border-bottom: 1rpx solid #fafafa;
}
.related_video_box .title {
    padding: 0 20rpx;
    margin-top: 20rpx;
}
.related_video_box .video_list {
    padding:0 20rpx;
    margin-top:20rpx;
}
.video_list .item {
    display: block;
    position: relative;
    padding: 20rpx 0 20rpx 270rpx;
    height: 160rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #fafafa;
}
.video_list .item .left_video_pic {
    position: absolute;
    left: 0;
    width: 250rpx;
    height: 160rpx;
    border-radius: 10rpx;
    overflow: hidden;
}

.video_list .item .right_video_info .video_title {
    display: block;
    height: 80rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
}
.video_list .item .right_video_info .video_data {
    overflow: hidden;
    line-height: 40rpx;
    color: #666;
}
.video_list .item .right_video_info .video_data text{
    display: inline-block;
    overflow: hidden;
    width: 50%;
    height: 40rpx;
    float: left;
    font-size: 24rpx;
}




.emit_danmu_box {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 80rpx;
    background-color: #333;
    position: relative;
    text-align: center;
}

.emit_danmu_box .left{
    flex: .25;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 5rpx;
}
.emit_danmu_box .select_color {
    flex: .25;
    text-align: center;
    height:60rpx;
    line-height: 60rpx;
}
.emit_danmu_box .right {
    flex: .5;
    overflow: hidden;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 5rpx;
}

.emit_danmu_box .danmu_text {
    flex: 1;
    height: 60rpx;
    background-color: #666;
    border-radius: 5rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    margin: 0 auto;
}
.emit_danmu_box .iconfont {
    color: #999;
}
.emit_danmu_box .position {
    height: 50rpx;
    display: inline-block;
    overflow: hidden;
}
.emit_danmu_box .position .select_position {
    display: none;
    position: absolute;
    background-color: #fff;
    top: 2rpx;
    z-index: 9999;
    border-radius: 10rpx;
    text-align: center;
    padding: 5rpx;
}

.palette-color-picker-button {
    position: static;
    display: inline-block;
    width: 50rpx ;
    height: 50rpx;
    border-radius: 50%;
    margin: 0 auto;
    background: url("data:image/png;base64,") center center no-repeat;
    background-size: cover;
    vertical-align: middle;
}